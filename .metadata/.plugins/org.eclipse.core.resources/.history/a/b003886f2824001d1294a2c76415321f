package UclnBcnn;

import java.util.Scanner;

public class main {
	public static int UCLN(Long a, Long b) {
		while(a != b) {
			if(a > b) {
				a = a - b;
			}
			else b = b - a;
		}
		return a;
	}
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int t = sc.nextInt();
		while(t != 0) {
			t--;
			int a = sc.nextInt();
			int b = sc.nextInt();
			System.out.println((a * b) / UCLN(a, b) + " " + UCLN(a, b));
		}
	}
}

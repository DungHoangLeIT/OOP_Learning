package SoXaCach;

import java.util.Scanner;

public class Main {
	static Scanner sc = new Scanner(System.in);
	static int used[] = new int[100];
	static int a[] = new int[100];
	static void Try(int i, int n) {
		for(int j = 1; j <= n; j++) {
			if(used[j] != 1) {
				a[i] = j;
				used[j] = 1;
				if(i == n) {
					for(int i1 = 1; i1 <= n; i1++) {
						System.out.println(a[i1]);
					}
					System.out.println();
				}
			}
			Try(i + 1, n);
			used[j] = 0;
		}
	}
	public static void main(String[] args) {
		int t = sc.nextInt();
		while(t != 0) {
			t -= 1;
			int n = sc.nextInt();
			Try(1,n);
		}
	}
}

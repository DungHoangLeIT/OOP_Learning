package TongPhanSO;

class PhanSo{
	private int x;
	private int y;
	public PhanSo(int x, int y) {
		this.x = x;
		this.y = y;
	}
	public int getX() {
		return this.x;
	}
	public void setX(int x) {
		this.x = x;
	}
	public int getY() {
		return y;
	}
	public void setY(int y) {
		this.y = y;
	}
	public int UCLN(int a, int b) {
		if(a % b == 0) return b;
		else return UCLN(b, a % b);
	}
	
	public Long BCNN(int a, int b) {
		return (a * b) / UCLN(a, b);
	}
}

public class main {
	
}

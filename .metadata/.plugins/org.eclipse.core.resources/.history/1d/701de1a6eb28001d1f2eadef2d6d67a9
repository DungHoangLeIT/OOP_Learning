package SoXaCach;

import java.util.*;

public class Main {
	static int a[] = new int[100];
	static int n;
	static int ok;
	
	
	static public void solve() {
		int i = n - 1;
		while(i >= 1 && a[i] > a[i + 1]) {
			i--;
		}
		if(i == 0) {
			ok = 1;
		}
		else {
			int j = n;
			while(a[i] > a[j]) j -= 1;
			int t = a[i];
			a[i] = a[j];
			a[j] = t;
			int l = i + 1;
			int r = n;
			while(l < r){
				int x = a[l];
				a[l] = a[r];
				a[l] = x;
				l++;
				r--;
			}
		}
	}
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		n = sc.nextInt();
		solve();
	}
}

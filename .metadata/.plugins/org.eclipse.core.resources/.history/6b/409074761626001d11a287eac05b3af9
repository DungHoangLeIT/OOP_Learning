package TongPhanSO;

import java.util.Scanner;

class PhanSo{
	private int x;
	private int y;
	public PhanSo(int x, int y) {
		this.x = x;
		this.y = y;
	}
	public int getX() {
		return this.x;
	}
	public void setX(int x) {
		this.x = x;
	}
	public int getY() {
		return y;
	}
	public void setY(int y) {
		this.y = y;
	}
	public int UCLN(int a, int b) {
		if(a % b == 0) return b;
		else return UCLN(b, a % b);
	}
	
	public int BCNN(int a, int b) {
		return (int) ((a * b) / UCLN(a, b));
	}
	public void RutGon(int a, int b) {
		int h = this.x;
		x = x / UCLN(x, y);
		y = y / UCLN(h, y);
	}
	public void solve(int a, int a1, int b, int b1) {
		Long h = BCNN(a1, b1);
		
	}
}


public class main {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		PhanSo x = new PhanSo(sc.nextInt(), sc.nextInt());
		PhanSo y = new PhanSo(sc.nextInt(), sc.nextInt());
		
	}
}

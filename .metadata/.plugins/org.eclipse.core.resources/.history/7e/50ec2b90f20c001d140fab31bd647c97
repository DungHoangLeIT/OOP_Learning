package huongDoiTuong;

import java.util.Scanner;

public class Main {
	public static void main(String[] args) {
		String n;
		double w;
		double p;
		Scanner sc = new Scanner(System.in);
		n = sc.nextLine();
		w = sc.nextDouble();
		p = sc.nextDouble();
		int x = sc.nextInt();
		HoaDonCafe HD = new HoaDonCafe(n, w, p);
		HD.setName("ADU");
		System.out.println(HD.getName());
		System.out.println(HD.SumOfPrice());
		System.out.println(HD.GiamGia(x));
	}
}
